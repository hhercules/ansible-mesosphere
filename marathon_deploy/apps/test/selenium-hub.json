{
  "id": "selenium-hub",
  "cpus": 1.0,
  "mem": 1024,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "selenium/hub:latest",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 4444, "hostPort": 4444, "servicePort": 0, "protocol": "tcp" }
      ]
    }
  }
}
